{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "linebreak-style": [
      true,
      "LF"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "no-console": [
      false,
      "log"
    ],
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
